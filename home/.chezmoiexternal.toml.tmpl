[".config/emacs"]
  type = "git-repo"
  url = "https://github.com/doomemacs/doomemacs.git"
  [".config/emacs".clone]
    args = ["--depth=1"]
[".tmux/plugins/tpm"]
  type = "git-repo"
  url = "https://github.com/tmux-plugins/tpm.git"
  refreshPeriod = "168h"
  [".tmux/plugins/tpm".clone]
    args = ["--depth=1"]
